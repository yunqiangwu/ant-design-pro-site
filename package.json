    "parallelshell": "^3.0.1",
    "react-copy-to-clipboard": "^5.0.0",
    "react-document-title": "^2.0.3",
    "react-github-button": "^0.1.11",
    "react-intl": "^2.3.0"
  },
  "devDependencies": {
    "atool-build": "1.0.2",
    "autod": "^2.10.1",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^19.0.0",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "babel-plugin-import": "^1.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "bisheng": "^0.26.0",
    "bisheng-plugin-antd": "^0.15.0",
    "bisheng-plugin-description": "^0.1.1",
    "bisheng-plugin-react": "^0.5.0",
    "bisheng-plugin-toc": "^0.4.0",
    "classnames": "^2.2.5",
    "commander": "~2.8.1",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.1",
    "css-split-webpack-plugin": "^0.2.3",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "latest",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-react": "^7.0.1",
    "eslint-tinker": "^0.4.1",
    "extract-text-webpack-plugin": "1.0.1",
    "fs-extra": "^4.0.0",
    "g-cloud": "^1.0.2-beta",
    "g2": "^2.3.12",
    "g2-plugin-slider": "^1.2.1",
    "gh-pages": "^1.0.0",
    "npm-run-all": "^4.0.2",
    "optimize-css-assets-webpack-plugin": "2.0.0",
    "pre-commit": "^1.2.2",
    "rc-queue-anim": "^1.3.1",
    "rc-scroll-anim": "^2.0.2",
    "rc-tween-one": "^1.5.3",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "shelljs": "^0.7.8",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "webpack": "^3.6.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development bisheng start -c ./site/bisheng.config.js --no-livereload",
    "bisheng-build": "cross-env NODE_ENV=production bisheng build -c ./site/bisheng.config.js",
    "scaffold-start": "cd ./scaffold && BROWSER=none npm start",
    "scaffold-move": "./tool/bin/let -m",
    "scaffold-build": "cd ./scaffold && npm run build && ../tool/bin/let -b",
    "scaffold-build:static": "./tool/bin/let -s && cd ./scaffold && npm run build && ../tool/bin/let -b",
    "scaffold-install": "cd ./scaffold && ../tool/bin/i",
    "preinstall": "cd ./scaffold && ../tool/bin/i",
    "site": "rm -rf _site && npm run bisheng-build && bash deploy_site.sh && echo gh-pages -d _site",
    "components-publish": "./tool/bin/let -c && cd ./config/components && npm publish",
    "clean": "rm -rf node_modules && cd ./scaffold && rm -rf node_modules",
    "lint": "npm run lint:es && npm run lint:style && npm run lint:demo",
    "lint:es": "eslint ./ --ext '.js,.jsx'",
    "lint:style": "stylelint \"site/**/*.less\" --syntax less",
    "lint:demo": "cross-env RUN_ENV=DEMO eslint ./ --ext '.md' -c ./.eslintrc.js",
    "test": "npm run lint"
  },
  "pre-commit": [
    "lint"
  ]
}
